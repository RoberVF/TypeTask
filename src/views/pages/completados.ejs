<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tareas Terminadas</title>
    <%- include('../utils/links.ejs') %>
</head>

<body>
    <a href="/">Volver</a>
    <% completed.forEach((i) => { %>
        <div class="card">
            <div class="card-header">
                <%= i.asignatura %>
            </div>
            <div class="card-body">
                <h5 class="card-title">
                    <%= i.nombre %>
                </h5>
                <p class="card-text">
                    <%= i.descripcion %>
                </p>
            </div>
        </div>
        <% }) %>
</body>

</html>